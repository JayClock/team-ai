{
  "name": "@hateoas-ts/resource",
  "version": "1.4.0",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.es.js"
    },
    "./zod": {
      "types": "./dist/zod.d.ts",
      "import": "./dist/zod.es.js",
      "require": "./dist/zod.cjs.js",
      "default": "./dist/zod.es.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "homepage": "https://jayclock.github.io/team-ai/packages/resource/",
  "repository": {
    "type": "git",
    "url": "https://github.com/JayClock/team-ai.git",
    "directory": "packages/resource"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@standard-schema/spec": "^1.1.0",
    "hal-types": "^2.0.0",
    "http-link-header": "^1.1.3",
    "inversify": "^7.10.4",
    "query-string": "^9.3.1",
    "url-template": "^3.1.1",
    "vitest": "4.0.15",
    "zod": "^4.3.6",
    "immer": "^11.1.4"
  },
  "devDependencies": {
    "@types/http-link-header": "^1.0.7",
    "msw": "^2.12.4"
  },
  "nx": {
    "targets": {
      "docs": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npx typedoc",
          "cwd": "packages/resource"
        },
        "outputs": [
          "{projectRoot}/docs"
        ]
      },
      "docs:validate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "npx typedoc --emit none",
          "cwd": "packages/resource"
        }
      }
    }
  }
}
